class Solution {
public:
    bool isSubsequence(string s, string t) 
    {
        int cnt=0,k=0;
            for(int j=0;j<t.length();j++)
            {
                if(s[k] == t[j])
                {
                    cnt++;
                    k++;
                }
            }
        if(cnt==s.length())
            return true;
        return false;
        
    }
};
